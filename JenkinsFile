pipeline {
  agent {
    docker { 
        image 'ubuntu:latest' 
        }
  }
  
 stages {
     stage('Checkout') {
            steps {
                git credentialsId: 'jenkins', url: 'https://github.com/zprgx/dockerized-django.git'
                echo 'CheckOut Success'
            }
        }
        stage('Install Requirements'){
            steps{
            sh "apt install python3 python3-pip"
            echo 'Done'
            }
        }
 }
}
